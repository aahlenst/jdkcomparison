{
	"name": "jdkcomparison",
	"private": true,
	"scripts": {
		"build": "npm run lint && npm run test && next build",
		"component": "cypress open --component --browser firefox",
		"component:headless": "cypress run --component --browser firefox",
		"cypress": "cypress open",
		"dev": "next dev",
		"e2e": "cypress open --e2e --browser firefox",
		"e2e:headless": "cypress run --e2e --browser firefox",
		"jest": "jest --transformIgnorePatterns \"node_modules/(?!micromark)/\"",
		"lint": "next lint",
		"start": "next start",
		"test": "npm run jest && npm run component:headless"
	},
	"dependencies": {
		"@headlessui/react": "1.7.13",
		"immer": "9.0.19",
		"micromark": "3.1.0",
		"next": "13.2.3",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"use-immer": "0.8.1"
	},
	"devDependencies": {
		"@jest/globals": "29.4.3",
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@tailwindcss/forms": "0.5.3",
		"@tailwindcss/typography": "0.5.9",
		"@types/node": "18.14.2",
		"@types/react": "18.0.28",
		"@types/react-dom": "18.0.11",
		"autoprefixer": "10.4.14",
		"cypress": "12.7.0",
		"eslint": "8.35.0",
		"eslint-config-next": "13.2.3",
		"jest": "29.4.3",
		"postcss": "8.4.21",
		"tailwindcss": "3.2.7",
		"typescript": "4.9.5"
	}
}
